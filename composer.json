        "laravel-enso",
        "comments",
        "comments-manager",
        "comments"
    ],
    "homepage": "https://github.com/laravel-enso/comments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
